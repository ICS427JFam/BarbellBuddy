{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "linebreak-style": "off",
    "max-len": "off",
    "no-console": "off",
    "react/jsx-props-no-spreading": "off",
    "no-unused-vars": "warn",
    "react/jsx-tag-spacing": "off",
    "no-plusplus": "off",
    "quote-props": "off",
    "react/jsx-one-expression-per-line": "off",
    "object-curly-newline": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-param-reassign": "off"
  }
}
